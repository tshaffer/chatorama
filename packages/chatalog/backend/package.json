{
  "name": "chatalog-backend",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently -k -n server,typecheck -c auto \"tsx watch src/server.ts\" \"tsc --noEmit --watch\"",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "seed": "tsx scripts/seedFromData.ts",
    "seed:reset": "RESET_DB=1 tsx scripts/seedFromData.ts",
    "backfill:docKind": "tsx src/scripts/backfillDocKind.ts",
    "audit:downloads": "tsx src/scripts/auditChatworthyDownloads.ts",
    "audit:aberrant": "tsx src/scripts/detectAberrantTurnBlocks.ts --out audit-aberrant-exports.json",
    "import:baseline": "tsx scripts/importChatRegistriesFromConversations.ts",
    "unreviewed:html": "tsx scripts/generateUnreviewedChatsHtml.ts",
    "smoke:pdf-text": "tsx src/scripts/smokePdfText.ts",
    "backfill:google-doc-urls": "tsx src/scripts/backfillGoogleDocDocsUrl.ts",
    "backfill:google-doc-embeddings": "tsx src/scripts/backfillGoogleDocEmbeddings.ts"
  },
  "dependencies": {
    "@chatorama/chatalog-shared": "file:../shared",
    "@types/unzipper": "^0.10.11",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "image-size": "^1.1.1",
    "mongoose": "^8.19.2",
    "multer": "^2.0.2",
    "openai": "^4.0.0",
    "pdf-parse": "^2.4.5",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@chatorama/chatalog-shared": "file:../shared",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "concurrently": "^9.2.1",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20"
  }
}
